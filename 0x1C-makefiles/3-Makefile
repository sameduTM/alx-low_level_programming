# -*- MakeFile -*-

#target: dependencies
#	action

CC=gcc
SRC=main.c school.c
OBJ=$(SRC:.c=.o)
NAME=school
RM=rm -f

all: $(NAME)

clean:
	$(RM) $(NAME) *~

oclean:
	$(RM) $(OBJ)

fclean: clean oclean
	$(RM) $(NAME)

re: clean all

$(NAME): $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

%.o: %.c
	$(CC) -c -o $@ $<

.PHONY: all clean oclean fclean re
